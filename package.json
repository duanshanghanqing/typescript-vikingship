    "axios": "^0.28.0",
    "classnames": "^2.2.6",
    "react-mixin": "^5.0.0",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
