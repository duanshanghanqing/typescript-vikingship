    "autoprefixer": "^9.8.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "firebase-functions-test": "^0.2.1",
    "html-webpack-plugin": "^4.3.0",
